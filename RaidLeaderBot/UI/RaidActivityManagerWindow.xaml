    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="396"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="106"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <GroupBox x:Name="ActivitySettingsGroupBox" Header="Activity Settings" HorizontalAlignment="Stretch">
            <Grid>
                <Button x:Name="SaveConfigButton"
                        Content="Save" 
                        Command="{Binding SaveConfigCommand}" 
                        Width="55"/>
                <ComboBox Name="PresetSelectorComboBox" 
                          IsEnabled="{Binding IsRunning, Converter={StaticResource InverseBooleanConverter}}" 
                          SelectedValue="{Binding ActivityPresetIndex}"
                          ItemsSource="{Binding ActivityPresetIndexes}" 
                          HorizontalContentAlignment="Left" 
                          VerticalContentAlignment="Center" 
                          d:SelectedIndex="0" 
                          Height="31" 
                          Margin="59,0,0,0" 
                          Width="55"/>
                <Button x:Name="AddPresetButton" 
                        IsEnabled="{Binding CanAddPreset}" 
                        Content="Add" 
                        Margin="334,0,0,0" 
                        Width="55"
                        Command="{Binding AddPresetCommand}"/>
                <Button x:Name="RemovePresetButton" 
                        IsEnabled="{Binding CanRemovePreset}" 
                        Content="Remove" 
                        d:IsEnabled="false" 
                        Margin="274,0,0,0" 
                        Width="55"
                        Command="{Binding RemovePresetCommand}"/>
                <Line X1="0" 
                      X2="386" 
                      Y1="42" 
                      Y2="42"  
                      Stroke="Black" 
                      StrokeThickness="1"></Line>
                <Button x:Name="AddRaidButton" 
                        IsEnabled="{Binding CanAddRaid}" 
                        Content="Add" 
                        Margin="334,51,0,0" 
                        Width="55"
                        Command="{Binding AddRaidCommand}"/>
                <Button x:Name="RemoveRaidButton" 
                        IsEnabled="{Binding CanRemoveRaid}" 
                        Content="Remove" 
                        d:IsEnabled="false" 
                        Margin="274,51,0,0" 
                        Width="55"
                        Command="{Binding RemoveRaidCommand}"/>
            </Grid>
        </GroupBox>
        <StackPanel Grid.Row="1">
            <ItemsControl ItemsSource="{Binding RaidPresetViewModels}">
                <d:ItemsControl.ItemsSource>
                    <x:Array Type="{x:Type local:RaidLeaderViewModel}">
                        <local:RaidLeaderViewModel CurrentActivity="WarsongGulch19" 
                                                   ShouldRun="True" 
                                                   IsFocused="True"
                                                   IsAlliance="True"
                                                   IsHorde="False"
                                                   RaidLeaderPortNumber="8089"/>
                        <local:RaidLeaderViewModel CurrentActivity="WarsongGulch19" 
                                                   ShouldRun="False" 
                                                   IsFocused="False"
                                                   IsAlliance="False"
                                                   IsHorde="True"
                                                   RaidLeaderPortNumber="8090"/>
                    </x:Array>
                </d:ItemsControl.ItemsSource>
                <ItemsControl.ItemTemplate>
                    <DataTemplate DataType="{x:Type local:RaidLeaderViewModel}">
                        <GroupBox Header="Party/Raid Settings" Height="90" MouseLeftButtonDown="RaidPresetGroupBox_MouseLeftClick">
                            <GroupBox.Style>
                                <Style TargetType="GroupBox">
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding IsFocused}" Value="True">
                                            <Setter Property="Foreground" Value="OrangeRed"/>
                                            <Setter Property="Background" Value="DarkOrange"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding IsFocused}" Value="False">
                                            <Setter Property="Foreground" Value="Black"/>
                                            <Setter Property="Background" Value="White"/>
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </GroupBox.Style>
                            <Grid>
                                <Button x:Name="StartAllBotsButton" 
                                        IsEnabled="{Binding CanStart}"
                                        Content="Start" 
                                        Command="{Binding StartAllRaidMembersCommand}" 
                                        Width="55"/>
                                <Button x:Name="StopAllBotsButton" 
                                        IsEnabled="{Binding CanStop}" 
                                        Content="Stop"  
                                        d:IsEnabled="False" 
                                        Margin="59,0,0,0"
                                        Command="{Binding StopAllRaidMembersCommand}" 
                                        Width="55"/>
                                <Button x:Name="MaxRaidSizeButton" 
                                        Width="55" 
                                        IsEnabled="{Binding CanAddMember}" 
                                        Content="Max" 
                                        Command="{Binding SetMaxRaidSizeCommand}" HorizontalAlignment="Right"/>
                                <Button x:Name="AddRaidMemberButton" 
                                        Margin="0,36,0,0"
                                        Width="55"
                                        IsEnabled="{Binding CanAddMember}" 
                                        Content="+" 
                                        Command="{Binding AddRaidMemberCommand}" 
                                        HorizontalAlignment="Right"/>
                                <Button x:Name="RemoveRaidMemberButton" 
                                        Margin="0,36,55,0" 
                                        IsEnabled="{Binding CanRemoveMember}" 
                                        Content="-" d:IsEnabled="false" 
                                        HorizontalAlignment="Right" 
                                        Width="55"
                                        Command="{Binding RemoveRaidMemberCommand}"/>
                                <ComboBox x:Name="InstanceComboBox" 
                                          Margin="0,36,0,0" 
                                          Height="31" 
                                          Width="224" 
                                          IsEnabled="{Binding IsFocused}" 
                                          VerticalContentAlignment="Center" 
                                          HorizontalContentAlignment="Left" 
                                          ItemsSource="{Binding Path=EnumActivityTypes}"
                                          SelectedValue="{Binding CurrentActivity}">
                                    <ComboBox.ItemTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding Converter={StaticResource EnumDescriptionConverter}}" />
                                        </DataTemplate>
                                    </ComboBox.ItemTemplate>
                                </ComboBox>
                                <RadioButton
                                    Margin="164,0,0,0"
                                    IsEnabled="{Binding IsFocused}"
                                    IsChecked="{Binding IsAlliance}">Alliance</RadioButton>
                                <RadioButton
                                    Margin="164,15,0,0"
                                    IsEnabled="{Binding IsFocused}"
                                    IsChecked="{Binding IsHorde}">Horde</RadioButton>
                                <TextBox Text="{Binding RaidLeaderPortNumber}" Width="50" Margin="229,0,0,0"/>
                            </Grid>
                        </GroupBox>
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>
        </StackPanel>
        <ScrollViewer Grid.Column="1" Margin="0,0,0,0" VerticalScrollBarVisibility="Hidden" Grid.RowSpan="2">
            <ItemsControl ItemsSource="{Binding RaidMemberViewModels}">
                <d:ItemsControl.ItemsSource>
                    <x:Array Type="{x:Type local:RaidMemberViewModel}">
                        <local:RaidMemberViewModel Header="Player 1 - Felhand" 
                                                   IsFocused="True"
                                                   Race="Tauren"
                                                   Class="Warrior"/>
                        <local:RaidMemberViewModel Header="Player 2 - Jmr" 
                                                   IsFocused="False"
                                                   Race="Undead"
                                                   Class="Priest"/>
                    </x:Array>
                </d:ItemsControl.ItemsSource>
                <ItemsControl.ItemTemplate>
                    <DataTemplate DataType="{x:Type local:RaidMemberViewModel}">
                        <GroupBox Header="{Binding Header}" HorizontalAlignment="Stretch" MouseLeftButtonDown="MemberPresetGroupBox_MouseLeftClick">
                            <GroupBox.Style>
                                <Style TargetType="GroupBox">
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding IsFocused}" Value="True">
                                            <Setter Property="Foreground" Value="OrangeRed"/>
                                            <Setter Property="Background" Value="DarkOrange"/>
                                            <Setter Property="Height" Value="516"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding IsFocused}" Value="False">
                                            <Setter Property="Foreground" Value="Black"/>
                                            <Setter Property="Background" Value="White"/>
                                            <Setter Property="Height" Value="54"/>
                                        </DataTrigger>
                                    </Style.Triggers>
